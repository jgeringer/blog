    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "postcss-import-resolver": "^2.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.1.1",
    "postcss-simple-vars": "^5.0.2",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
