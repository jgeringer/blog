    "esm": "^3.2.25",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^2.1.13",
    "gatsby-plugin-google-tagmanager": "^2.4.0",
